{
  "permissions": {
    "allow": [
      "Bash(curl:*)",
      "Bash(cat:*)",
      "WebFetch(domain:api.github.com)",
      "Bash(grep:*)",
      "Bash(npm run build:*)",
      "Bash(npm install:*)",
      "Bash(npx drizzle-kit:*)",
      "Bash(ls:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(xargs ls:*)",
      "Bash(find:*)",
      "Bash(node -e:*)",
      "Bash(yes:*)",
      "Bash(while read file)",
      "Bash(do if ! grep -q \"export const dynamic\" \"$file\")",
      "Bash(then echo \"Missing dynamic: $file\" fi done)"
    ]
  }
}
